@Article{Kim2012,
  Title                    = {AGREEMENT BETWEEN THE WHITE MATTER CONNECTIVITY BASED ON THE TENSOR-BASED MORPHOMETRY AND THE VOLUMETRIC WHITE MATTER PARCELLATIONS BASED ON DIFFUSION TENSOR IMAGING.},
